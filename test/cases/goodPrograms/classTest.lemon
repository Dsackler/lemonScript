receive Car from Ferrari

Limon Car
BEGIN JUICING
    plant(pulp name ,slice year)
        BEGIN JUICING
        pulp thisLemon = name
        thisLemon.year = year
        END JUICING

    When life gives you lemons try slice getYear()
        BEGIN JUICING
        you get lemonade and thisLemon.year
        END JUICING

END JUICING
Car myCar = seed Car("Ford",2014)

Limon Ferrari branches Car
BEGIN JUICING
        plant(slice year)
        BEGIN JUICING
        thisLemon.name = "F8 Spider"
        thisLemon.year = year
        END JUICING
END JUICING